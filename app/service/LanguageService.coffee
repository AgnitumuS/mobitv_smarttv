  convert: (text, type = 'vi')-> # type : vi || en
    self = @
    tempStr = ''
    if typeof text == 'object'
      i = 0
      while i < text.length
        return '' unless text[i]
        # text[i] = text[i].toLowerCase()
        item = self.data[text[i]]
        if type and item and item[type]
          tempStr += item[type]
        else
          tempStr += text[i]
        if i <= text.length - 1
          tempStr += ' '
        i++
      return tempStr
    else
      return '' unless text
      # text = text.toLowerCase()
      item = self.data[text]
      if type and item and item[type]
        return item[type]
      return text

