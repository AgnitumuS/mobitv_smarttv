div#player
div.player-page.animated.fadeIn
  div.gradient-top
  div.gradient-bottom
  div.player-loading
    img.fa-spin(src='data:image/png;base64,' +
    '')

  div.player-header
    .player-title
      i.icon.ic-back.bt-back
      h1 {{item.knownAs}}
      p {{item.title}}

  div.player-bottom
    .player-control-layer
      .player-control
        a
          i.button-status.icon.ic-pause
          //i.icon.ic-play
        .indicator
          span.current-time 00:00
        .bar-container
          .bar-bg
            .bar-hover.buffer-bar(style='width:0')
          .bar-pointer
        .indicator.last
          span.duration-time 00:00:00

    .player-bt
      | {{#each buttonActions}}
      a
        i.icon(class='{{icon}}')
        span {{language title}}
      | {{/each}}
  .player-snapshot

  #player-setting

  .player-sub(style="display:none")
    p.player-sub-wrapper
      span.player-sub-detail {{language 'account-subtitle-sample'}}



